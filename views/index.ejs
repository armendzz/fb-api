  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>KF Arsimi FB</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="style.css">
  </head>
  <body>

   



    <div class="container">
    <%   postt.data.forEach((post) => { %>


      <div class="card" id="card-header">
        <div class="card-header" id="card-header1">
          KF ARSIMI
        </div>
        <div class="card-body">
          <p class="card-text"> <%= post.created_time %> </p>
          <h5 class="card-title"><%= post.message %></h5>
          <% if(post.attachments === undefined){ %>
            <% } else{ %>  
              <% if(post.attachments.data[0].subattachments === undefined){ %>
                  <% if(post.attachments.data[0].media.source === undefined){ %>
                  <p> <img src="<%= post.attachments.data[0].media.image.src %>"> </p>
                  <% } else{ %>  
                  <p>  <video width="400" controls>
                      <source src="<%= post.attachments.data[0].media.source %>" type="video/mp4">
                      Your browser does not support HTML video.
                    </video></p> <% }; %>  
                <% } else{ %>  
                  <% post.attachments.data[0].subattachments.data.forEach(function(image) { %>
                    <p> <img src="<%= image.media.image.src %>" class="img-fluid"> </p>
                      <% }) %>
               <% } %>
           <% } %>
                    
        </div>
      </div>      
     
      <%  }); %>
 
    </div>
